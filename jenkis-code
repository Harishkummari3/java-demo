pipeline {
    agent any
    environment {
        DISABLE_AUTH = 'true'
        DB_ENGINE    = 'sqlite'
    }
    stages {
        stage('Build') {
            steps {
                git branch: 'main', url: 'https://github.com/Harishkummari3/java-demo'
                
            }
        }
        stage('Hello'){
            steps{
            echo "hello world"
        }
        }
         

    
        stage('compile') {
            steps {
                echo "Database engine is ${DB_ENGINE}"
                echo "DISABLE_AUTH is ${DISABLE_AUTH}"
                echo "printenv"
            }
        }
        
    }
}
